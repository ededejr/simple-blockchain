{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES2020"],
    "declaration": true,
    "outDir": "./build",
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,                  
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "tests/*",
    "src/launch.ts"
  ]
}
